# Copy to .env and fill with your Twilio credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+17343367101

# Agent Provisioning API (optional)
# Enable automated agent creation via HTTP API
OPENCLAW_PROVISIONING_ENABLED=false
# Rate limit for provisioning requests (requests per minute per key)
OPENCLAW_PROVISIONING_RATE_LIMIT=10
# Master AI provider keys for auto-provisioning (optional)
# ANTHROPIC_MASTER_KEY=sk-ant-api03-...
# OPENAI_MASTER_KEY=sk-...
